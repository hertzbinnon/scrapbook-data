<script type="text/javascript">
    $(function () {
        $("#btnSubmit").unbind("click");
        $("#btnSubmit").click(function () {
            search();
        });

        $("#frmSearch").submit(function () {
            search();
            return false;
        });
        function search() {
            if ($("#inputSearch").val() == "") {
                alert("请录入搜索关键词！");
                return false;
            }
            var url = CFG.SO_URL + "?q=" + encodeURIComponent($("#inputSearch").val())+'&t=blog&u='+username;
            // window.location.href = url;
            window.open(url)
        }
    });
</script>

                                
