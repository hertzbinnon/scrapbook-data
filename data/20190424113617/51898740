    }
    function permissionCheck(dom){
      var returnData = false;
      switch (true) {
        case currentUserPermission && userPermission:
          clearReadMoreBtn(dom)
          returnData = true;
          break;
        case userPermission:
          dom.text('VIP阅读');
          returnData = false;
          break;
        default:
          
      }
      return returnData;
    }
    window.csdn = window.csdn ? window.csdn : {};
    window.csdn.articleCollection = collection;
    function clearReadMoreBtn(dom) {
      if(userPermission && !(dom instanceof jQuery)) {
        window.open('https://mall.csdn.net/vip_code?utm_source=brv')
        return false;
      }
      dom = dom instanceof jQuery ? dom : $(this);
      $("div.article_content").removeAttr("style");
      dom.parent().remove();
    }
    function setArticleH(btnReadmore,posi){
      var winH = $(window).height();
      var articleBox = $("div.article_content");
      var artH = articleBox.height();
      if(artH > winH*posi){
        articleBox.css({
          'height':winH*posi+'px',
          'overflow':'hidden'
        })
        btnReadmore.click(clearReadMoreBtn)
      }else{
        btnReadmore.parent().remove()
      }
    }
    var btnReadmore = $("#btn-readmore");
    $('.article-footer-bookmark-btn').click(window.csdn.articleCollection)
    if(btnReadmore.length>0){
      if (permissionCheck(btnReadmore)) {
        return false;
      }
      if(currentUserName){
        setArticleH(btnReadmore,3);
      }else{
        setArticleH(btnReadmore,1.2);
      }
    }else{
      $('.hide-article-box').addClass('hide-article-style');
    }
  })()
</script>
